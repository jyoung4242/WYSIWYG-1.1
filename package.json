{
  "name": "ex_ide",
  "version": "1.0.0",
  "main": "electron/main.ts",
  "scripts": {
    "build": "vite build && tsc -p tsconfig.electron.json",
    "start": "npm run build && electron dist-electron/main.js",
    "dev": "concurrently -k \"vite\" \"tsc -p tsconfig.electron.json --watch && electron dist-electron/main.js\""
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^8.2.2",
    "electron": "^31.7.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.20",
    "vite-plugin-static-copy": "^3.1.2"
  },
  "dependencies": {
    "dotenv": "^17.2.2"
  }
}
